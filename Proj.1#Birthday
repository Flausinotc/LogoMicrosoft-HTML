// começo do código

function birthday(s, d, m){

let count = 0;
let sum = 0;
for(let tjf =0; tjf <= (s.length -m); tjf +=1){
    for (let tjf2 = 0; tjf2 < m; tjf2 += 1){
        sum = sum + s [tjf + tjf2];
    }
    if (sum ===d) {
count += 1;
    }

    sum =0;
    }

    return count;


};

console.log(birthday([2,2,1,3,2],4,2)); //Imprimir resultado (que no caso tem que ser ===2)

